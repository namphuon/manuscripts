mendeley-groups = {Virus/ecDNA},
number = {7643},
pages = {122--125},
pmid = {11507039},
publisher = {Nature Publishing Group},
title = {{Extrachromosomal oncogene amplification drives tumour evolution and genetic heterogeneity}},
OPTurl = {http://www.nature.com/doifinder/10.1038/nature21356},
volume = {543},
