title = {{VERSE: a novel approach to detect virus integration in host genomes through reference genome customization.}},
OPTurl = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed{\&}id=25699093{\&}retmode=ref{\&}cmd=prlinks$\backslash$npapers3://publication/doi/10.1186/s13073-015-0126-6$\backslash$nfile:///Users/Joel/Documents/Library.papers3/Articles/2015/Wang/Genome Med 2015 Wang.pdf},
