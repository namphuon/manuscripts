isbn = {0950-9232},
issn = {0950-9232},
journal = {Oncogene},
keywords = {Carcinoma,Carcinoma: genetics,Carcinoma: metabolism,Carcinoma: virology,Cell Line, Tumor,DNA, Viral,DNA, Viral: metabolism,Female,Gene Expression Regulation, Neoplastic,Genetic Markers,Human papillomavirus 16,Human papillomavirus 16: genetics,Human papillomavirus 16: pathogenicity,Human papillomavirus 18,Human papillomavirus 18: genetics,Human papillomavirus 18: pathogenicity,Humans,Male,Penile Neoplasms,Penile Neoplasms: genetics,Penile Neoplasms: metabolism,Penile Neoplasms: virology,Proto-Oncogene Proteins c-myc,Proto-Oncogene Proteins c-myc: genetics,Proto-Oncogene Proteins c-myc: metabolism,Uterine Cervical Neoplasms,Uterine Cervical Neoplasms: genetics,Uterine Cervical Neoplasms: metabolism,Uterine Cervical Neoplasms: virology,Virus Integration,Virus Integration: genetics},
